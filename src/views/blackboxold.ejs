<script>
const form = document.getElementById('form1');
const priceError = document.getElementById('err1');
const review_scoreError = document.getElementById('err2');
const product_category_nameError = document.getElementById('err3');
const data = document.getElementById('data');
form.addEventListener('submit', async (e) => {
    e.preventDefault();
      // reset errors
      priceError.textContent = '';
      review_scoreError.textContent = '';
      product_category_nameError.textContent = '';
      data.textContent = '';
      // get values
    const price = form.price.value;
    const review_score = form.review_score.value;
    const product_category_name = form.product_category_name.value;
    try {
      const res = await fetch('/api/tools/BlackBox', { 
        method: 'POST', 
        body: JSON.stringify({price, review_score, product_category_name}),
        headers: {'Content-Type': 'application/json'}
      });
      const data = await res.json();
      console.log(data);
        if (data.errors) {
          console.log(data.errors);
          priceError.textContent = data.errors.price;
          review_scoreError.textContent = data.errors.review_score;
          product_category_nameError.textContent = data.errors.product_category_name;
        }

        if (data) {

          console.log(data);
          data.textContent = data;
        }
    }
    catch (err) {
        console.log(err);
    }
  
  })
</script>




