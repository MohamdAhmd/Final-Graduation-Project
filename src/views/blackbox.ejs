<script>
  const form = document.getElementById('contact');
  const data1 = document.getElementById('1');
  const data2 = document.getElementById('2');
  const data3 = document.getElementById('3');
  const data4 = document.getElementById('4');
  const data5 = document.getElementById('5');
  form.addEventListener('submit', async (e) => {
      e.preventDefault();
        // reset errors
        data1.textContent = '';
        data2.textContent = '';
        data3.textContent = '';
        data4.textContent = '';
        data5.textContent = '';
        // get values
      const min_price = Number(form.min_price.value);
      const max_price = Number(form.max_price.value);
      const review_score = form.review_score.value;
      const product_category_name = form.product_category_name.value;
      try {
        const res = await fetch('/api/tools/BlackBox', { 
          method: 'POST', 
          body: JSON.stringify({min_price, max_price, review_score, product_category_name}),
          headers: {'Content-Type': 'application/json'}
        });
        const data = await res.json();
          if (data.errors) {
            console.log(data.errors);
          }
          if (data) {
            document.getElementById('1').innerHTML = data.data.product_id 
            document.getElementById('2').innerHTML = data.data.product_category_name 
            document.getElementById('3').innerHTML = data.data.review_score
            document.getElementById('4').innerHTML = data.data.price 
            document.getElementById('5').innerHTML = data.data.counts 


            console.log(data);
          }
      }
      catch (err) {
          console.log(err);
      }    
    })
</script>      



